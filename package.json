{
  "name": "api-server",
  "version": "1.0.0",
  "description": "plating api server v3",
  "main": "index.js",
  "author": "duck",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_PATH=. NODE_ENV=development pm2 start index.js",
    "start:prod": "cross-env NODE_PATH=. NODE_ENV=production pm2 start index.js",
    "start:API_V3_DEV": "npm run start:dev -- --name API_V3_DEV --output NULL --error NULL",
    "start:API_V3_ALPHA": "npm run start:prod -- --name API_V3_ALPHA --output NULL --error NULL",
    "start:API_V3": "npm run start:prod -- --name API_V3",
    "stop:API_V3_DEV": "pm2 stop API_V3_DEV",
    "stop:API_V3_ALPHA": "pm2 stop API_V3_ALPHA",
    "stop:API_V3": "pm2 stop API_V3",
    "restart:API_V3_DEV": "pm2 restart API_V3_DEV",
    "restart:API_V3_ALPHA": "pm2 restart API_V3_ALPHA",
    "restart:API_V3": "pm2 restart API_V3",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development pm2-dev start index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test NODE_PATH=. mocha src/**/*.\\{spec\\,test\\}.js"
  },
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "aws-sdk": "^2.272.1",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "circular-json": "^0.5.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.1",
    "datetime-offset": "^0.3.0-beta",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "express-http-proxy": "^1.1.0",
    "helmet": "^3.9.0",
    "http-proxy-middleware": "^0.17.4",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.17",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.1",
    "pino": "^4.10.2",
    "qs": "^6.5.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.22.5",
    "swagger-ui": "^3.6.0",
    "swagger-ui-dist": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "sequelize-cli": "^3.2.0",
    "supertest": "^3.0.0"
  },
  "lint-staged": {
    "*.js": "lint"
  }
}
